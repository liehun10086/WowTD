    if ( not ( GetUnitUserData(GetEnteringUnit()) == udg_OutsideCounterClockwise ) ) then
        return false
    endif
    return true
endfunction

function Trig_OutsideTL_Actions takes nothing returns nothing
    if ( Trig_OutsideTL_Func002C() ) then
        set udg_CurrentlyOnMap = ( udg_CurrentlyOnMap + ( 225.00 / I2R(udg_NumberToSpawn[GetUnitLevel(GetEnteringUnit())]) ) )
        call SetUnitOwner( GetEnteringUnit(), Player(11), false )
        if ( Trig_OutsideTL_Func002Func004C() ) then
            call SetUnitUserData( GetEnteringUnit(), udg_OutsideCounterClockwise )
        else
            call SetUnitUserData( GetEnteringUnit(), udg_OutsideClockwise )
        endif
    else
        call DoNothing(  )
    endif
    if ( Trig_OutsideTL_Func003C() ) then
        set udg_TempPoint = GetRectCenter(gg_rct_OutsideLeft)
    else
        set udg_TempPoint = GetRectCenter(gg_rct_OutsideTop)
    endif
    call IssuePointOrderLocBJ( GetEnteringUnit(), "move", udg_TempPoint )
    call RemoveLocation (udg_TempPoint)
