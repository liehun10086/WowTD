//===========================================================================
function Trig_CountVotes_Func002A takes nothing returns nothing
    call DialogDisplayBJ( false, udg_DialogueBouton, GetEnumPlayer() )
endfunction

function Trig_CountVotes_Func005Func006C takes nothing returns boolean
    if ( not ( udg_HardCount >= udg_InsaneCount ) ) then
        return false
    endif
    return true
endfunction

function Trig_CountVotes_Func005C takes nothing returns boolean
    if ( not ( udg_TempInt >= udg_HardCount ) ) then
        return false
    endif
    if ( not ( udg_TempInt >= udg_InsaneCount ) ) then
        return false
    endif
    return true
endfunction

function Trig_CountVotes_Actions takes nothing returns nothing
    call SetMapFlag( MAP_USE_HANDICAPS, true )
    call ForForce( GetPlayersAll(), function Trig_CountVotes_Func002A )
    set bj_forLoopAIndex = 0
    set bj_forLoopAIndexEnd = 8
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        set udg_HardCount = ( udg_HardVotes[GetForLoopIndexA()] + udg_HardCount )
        set udg_InsaneCount = ( udg_InsaneVotes[GetForLoopIndexA()] + udg_InsaneCount )
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
    set udg_TempInt = ( CountPlayersInForceBJ(udg_ConnectedPlayers) - ( udg_HardCount + udg_InsaneCount ) )
    if ( Trig_CountVotes_Func005C() ) then
        call DisplayTextToForce( GetPlayersAll(), "TRIGSTR_1223" )
        call SetPlayerHandicapBJ( Player(11), 75.00 )
        call SetPlayerHandicapBJ( Player(10), 75.00 )
        set udg_TempPoint = GetRectCenter(GetPlayableMapRect())
        call RemoveLocation (udg_TempPoint)
    else
        if ( Trig_CountVotes_Func005Func006C() ) then
            call DisplayTextToForce( GetPlayersAll(), "TRIGSTR_1224" )
            set udg_TempPoint = GetRectCenter(GetPlayableMapRect())
            call SetPlayerHandicapBJ( Player(11), 100.00 )
            call SetPlayerHandicapBJ( Player(10), 100.00 )
            call RemoveLocation (udg_TempPoint)
        else
            call DisplayTextToForce( GetPlayersAll(), "TRIGSTR_1225" )
            set udg_TempPoint = GetRectCenter(GetPlayableMapRect())
            call SetPlayerHandicapBJ( Player(11), 125.00 )
            call SetPlayerHandicapBJ( Player(10), 125.00 )
