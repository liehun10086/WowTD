    if ( not ( GetUnitUserData(GetEnteringUnit()) == udg_OutsideToInside ) ) then
        return false
    endif
    return true
endfunction

function Trig_OutsideLeft_Actions takes nothing returns nothing
    // First we choose a direction.
    if ( Trig_OutsideLeft_Func003C() ) then
        if ( Trig_OutsideLeft_Func003Func001C() ) then
            call SetUnitUserData( GetEnteringUnit(), udg_OutsideClockwise )
        else
            call SetUnitUserData( GetEnteringUnit(), udg_OutsideToInside )
        endif
    else
        call DoNothing(  )
    endif
    if ( Trig_OutsideLeft_Func004C() ) then
        if ( Trig_OutsideLeft_Func004Func001C() ) then
            call SetUnitUserData( GetEnteringUnit(), udg_OutsideCounterClockwise )
        else
            call SetUnitUserData( GetEnteringUnit(), udg_OutsideToInside )
        endif
    else
        call DoNothing(  )
    endif
    if ( Trig_OutsideLeft_Func005C() ) then
        if ( Trig_OutsideLeft_Func005Func001C() ) then
            call SetUnitUserData( GetEnteringUnit(), udg_OutsideCounterClockwise )
        else
            call SetUnitUserData( GetEnteringUnit(), udg_OutsideClockwise )
        endif
    else
        call DoNothing(  )
    endif
    // Now we give the movement command, and if nescessary reset the direction flag for switches.
    if ( Trig_OutsideLeft_Func007C() ) then
        set udg_TempPoint = GetRectCenter(gg_rct_OutsideTL)
    else
        call DoNothing(  )
    endif
    if ( Trig_OutsideLeft_Func008C() ) then
        set udg_TempPoint = GetRectCenter(gg_rct_OutsideBL)
    else
        call DoNothing(  )
    endif
    if ( Trig_OutsideLeft_Func009C() ) then
        set udg_TempPoint = GetRectCenter(gg_rct_InsideLeft)
    else
        call DoNothing(  )
    endif
    call IssuePointOrderLocBJ( GetEnteringUnit(), "move", udg_TempPoint )
    call RemoveLocation (udg_TempPoint)
endfunction

