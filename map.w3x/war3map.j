function Trig_SpawnWaves_Func001Func002Func004A takes nothing returns nothing
    set udg_TempPoint = GetRandomLocInRect(udg_SpawnAreas[GetConvertedPlayerId(GetEnumPlayer())])
    call CreateNUnitsAtLoc( 1, udg_UnitToSpawn, Player(10), udg_TempPoint, bj_UNIT_FACING )
    call RemoveLocation (udg_TempPoint)
endfunction

function Trig_SpawnWaves_Func001Func002C takes nothing returns boolean
    if ( not ( udg_SpawningGroup == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_SpawnWaves_Func001C takes nothing returns boolean
    if ( not ( udg_SpawnCounter >= udg_SpawningCalc ) ) then
        return false
    endif
    if ( not ( udg_UnitToSpawn != 0 ) ) then
        return false
    endif
    return true
endfunction

function Trig_SpawnWaves_Actions takes nothing returns nothing
    if ( Trig_SpawnWaves_Func001C() ) then
        if ( Trig_SpawnWaves_Func001Func002C() ) then
            set udg_SpawningGroup = false
            call ForForce( udg_GroupOutside, function Trig_SpawnWaves_Func001Func002Func004A )
            set udg_NextSpawnTracker = ( udg_NextSpawnTracker + udg_SpawningRate )
            set udg_SpawningCalc = R2I(udg_NextSpawnTracker)
        else
            set udg_SpawningGroup = true
            call ForForce( udg_GroupInside, function Trig_SpawnWaves_Func001Func002Func002A )
        endif
        set udg_EmptySpawnCounter = ( udg_EmptySpawnCounter + 1 )
    else
        call DoNothing(  )
    endif
    set udg_SpawnCounter = ( udg_SpawnCounter + 1 )
endfunction

