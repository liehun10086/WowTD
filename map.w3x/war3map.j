function Trig_SetupWave_Func002C takes nothing returns boolean
    if ( not ( udg_LVL >= 37 ) ) then
        return false
    endif
    return true
endfunction

function Trig_SetupWave_Actions takes nothing returns nothing
    set udg_LVL = ( udg_LVL + 1 )
    if ( Trig_SetupWave_Func002C() ) then
        call TriggerExecute( gg_trg_Victory )
        return
    else
        set udg_TempString = ( "|c008000FFWave " + ( I2S(udg_LVL) + " of 36.|r" ) )
        call DisplayTextToForce( GetPlayersAll(), udg_TempString )
        call TriggerExecute( gg_trg_Next )
        call LeaderboardSetPlayerItemValueBJ( Player(9), udg_LEADERBOARD, udg_LVL )
        set udg_UnitToSpawn = udg_WaveUnits[udg_LVL]
        set udg_SpawningRate = ( 450.00 / I2R(udg_NumberToSpawn[udg_LVL]) )
        set udg_SpawnCounter = 0
        set udg_EmptySpawnCounter = 0
        set udg_NextSpawnTracker = 0.00
        set udg_SpawningCalc = R2I(udg_NextSpawnTracker)
    endif
endfunction

